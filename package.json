{
  "name": "nuxt-express",
  "version": "0.0.1",
  "description": "Nuxt.js with Express",
  "author": "David Royer",
  "private": true,
  "scripts": {
    "dev": "backpack dev",
    "build": "nuxt build && backpack build",
    "start": "cross-env NODE_ENV=production node dist/server/main.js",
    "generate": "nuxt generate",
    "build-server": "backpack build",
    "build-client": "nuxt build",
    "deploy:now": "now --public && now alias"
  },
  "dependencies": {
    "@nuxtjs/axios": "^5.0.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^1.0.2",
    "express": "^4.15.3",
    "nuxt": "^1.0.0"
  },
  "devDependencies": {
    "backpack-core": "^0.7.0",
    "cross-env": "^5.0.1",
    "nodemon": "^1.11.0"
  },
  "engines": {
    "node": ">=8.7.0"
  },
  "now": {
    "name": "nuxtqs-express",
    "alias": [
      "nuxtqs-express",
      "express.nuxtjs-quickstart.com"
    ]
  }
}
